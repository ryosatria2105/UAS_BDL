	<queryString language="SQL">
		<![CDATA[SELECT 
    p.id_penjualan,
    pg.nama_pegawai AS nama_kasir,
    pb.nama_pembeli,
    jp.nama_jenis AS metode,
    p.total,
    py.jumlah_bayar,
    (py.jumlah_bayar - p.total) AS kembalian,
    p.tanggal
FROM penjualan p
JOIN pegawai pg ON pg.id_pegawai = p.id_pegawai
LEFT JOIN pembeli pb ON pb.id_pembeli = p.id_pembeli
LEFT JOIN pembayaran py ON py.id_penjualan = p.id_penjualan
LEFT JOIN jenis_pembayaran jp ON jp.id_jenis = py.id_jenis
ORDER BY p.tanggal DESC;]]>
	</queryString>
