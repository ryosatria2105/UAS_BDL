	<queryString>
		<![CDATA[SELECT 
    r.id_restock,
    pg.nama_pegawai AS nama_admin,
    sp.nama_supplier AS nama_supplier,
    COALESCE((
        SELECT SUM(dr.jumlah)
        FROM detail_restock dr
        WHERE dr.id_restock = r.id_restock
    ), 0) AS total_item,
    r.tanggal
FROM restock r
JOIN pegawai pg ON pg.id_pegawai = r.id_pegawai
JOIN supplier sp ON sp.id_supplier = r.id_supplier
ORDER BY r.tanggal DESC;]]>
	</queryString>
