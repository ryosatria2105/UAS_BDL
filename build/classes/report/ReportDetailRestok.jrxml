	<parameter name="id_restok" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    dr.id_barang,
    b.nama_barang,
    dr.harga_beli,
    dr.jumlah,
    (dr.harga_beli * dr.jumlah) AS subtotal
FROM detail_restock dr
JOIN barang b ON b.id_barang = dr.id_barang
WHERE dr.id_restock = $P{id_restok}
ORDER BY dr.id_barang;]]>
	</queryString>
