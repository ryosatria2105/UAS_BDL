	<queryString language="SQL">
		<![CDATA[SELECT 
    p.id_pembayaran,
    p.id_penjualan,
    b.nama_pembeli,
    jp.nama_jenis AS metode,
    pj.total,
    p.jumlah_bayar,
    (p.jumlah_bayar - pj.total) AS kembalian,
    p.tanggal_pembayaran
FROM pembayaran p
JOIN penjualan pj ON pj.id_penjualan = p.id_penjualan
JOIN pembeli b ON b.id_pembeli = pj.id_pembeli
JOIN jenis_pembayaran jp ON jp.id_jenis = p.id_jenis
ORDER BY p.tanggal_pembayaran DESC;]]>
	</queryString>
