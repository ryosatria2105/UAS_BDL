	<queryString>
		<![CDATA[SELECT 
    p.id_pembayaran,
    p.id_penjualan,
    p.tanggal_pembayaran,
    p.jumlah_bayar,
    pj.total,
    (p.jumlah_bayar - pj.total) AS kembalian,
    jp.nama_jenis
FROM pembayaran p
JOIN penjualan pj ON p.id_penjualan = pj.id_penjualan
JOIN jenis_pembayaran jp ON p.id_jenis = jp.id_jenis
WHERE p.id_pembayaran = $P{id_pembayaran}]]>
	</queryString>
